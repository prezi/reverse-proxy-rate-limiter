{
  "log4js": {
    "appenders": {
      "console" : {
        "type": "console",
        "layout": {
          "type": "pattern",
          "pattern": "%d{ISO8601} %h %c %p %m%n"
        }
      }
    },
    "categories": {
      "default": {
        "appenders": ["console"],
        "level": "debug"
      }
    }
  }
}
